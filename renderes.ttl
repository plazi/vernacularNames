[] a r2h:Renderer;
    r2h:type rdfs:Resource;
    r2h:context gt:HtmlBody;
    r2h:mustache """
        {{{:render . r2h:Default}}}
    """.

[] a r2h:Renderer;
    r2h:type rdfs:Resource;
    r2h:context gt:HtmlHead;
    r2h:mustache """
        {{{:render . gt:HtmlTitle}}}
    """.

[] a r2h:Renderer;
    r2h:type rdfs:Resource;
    r2h:context gt:HtmlTitle;
    r2h:mustache """
        <title>Plazi Vernadular Names -  {{{.}}}</title>
    """.